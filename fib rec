#include <iostream>

int fibonacci(int n) {
    if (n <= 1) {
        return n;
    }
    return fibonacci(n - 1) + fibonacci(n - 2);
}

int main() {
    int number;
    std::cout << "Введите число: ";
    std::cin >> number;

    std::cout << "Число Фибоначчи для " << number << ": " << fibonacci(number) << std::endl;

    return 0;
}
